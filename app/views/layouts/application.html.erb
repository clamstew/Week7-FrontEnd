<!DOCTYPE html>
<html>
<head>
  <title>RailsApl</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  
  <%= csrf_meta_tags %>
</head>
<body>

<div id="templates">

  <script class="task" type="text/template">
    <label>
      <input class="check" type="checkbox" />
      {{ name }}
    </label>
  </script>

</div>

<%= yield %>

<%= render 'layouts/server_intel' %>
<%= yield :intel %>

<%= javascript_include_tag "application" %>
<%= yield :javascript %>

</body>
</html>
